<%- include('../partials/header') %>


<div>
    Team Info:
    <br>
<%= response.market %>
<%= response.name %>


<% if (response.players.length) { %>
    <% response.players.forEach(function(id) { %>
    <br>
    <%= id.full_name %>
    <a href="/playerinfo/<%= id.id %>">Player Profile</a>
    
    <%= id.position %>
    <%= id.jersey_number %>
    
    <% })}; %>




</div>


<%- include('../partials/footer') %>